{
  "name": "@xarenas107/nuxt-socket-io",
  "author": {
    "name": "Xavier Arenas",
    "email": "xarenas107@gmail.com"
  },
  "private": false,
  "version": "3.2.3",
  "description": "Nuxt module for socket.io",
  "keywords": [
    "websocket",
    "socket.io",
    "nuxt",
    "vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xarenas107/nuxt-socket.io"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "build": {
    "externals": [
      "socket.io-client",
      "socket.io",
      "defu"
    ]
  },
  "scripts": {
    "prepare": "nuxt-module-build prepare",
    "prepack": "nuxt-module-build build",
    "build": "nuxi build-module",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run prepack && changelogen --release && npm publish --access public && git push --follow-tags",
    "lint": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "h3": "^1.13.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.3.2",
    "@nuxt/eslint-config": "^1.3.0",
    "@nuxt/kit": "^3.16.2",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.16.2",
    "@nuxt/test-utils": "^3.15.1",
    "@pinia/nuxt": "^0.11.0",
    "@types/node": "^22.7.4",
    "changelogen": "^0.6.1",
    "crossws": "^0.3.1",
    "defu": "^6.1.4",
    "engine.io": "^6.6.2",
    "eslint": "^9.12.0",
    "nuxt": "^3.16.2",
    "vitest": "^3.1.1"
  },
  "overrides": {
    "vue": "latest"
  }
}
