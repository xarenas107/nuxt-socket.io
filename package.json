{
  "name": "@xarenas107/nuxt-socket.io",
  "author": {
    "name": "Xavier Arenas",
    "email": "xarenas107@gmail.com"
  },
  "private": false,
  "version": "1.8.15",
  "description": "Nuxt module for socket.io",
  "keywords": [
    "websocket",
    "socket.io",
    "nuxt",
    "vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xarenas107/nuxt-socket.io"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "build": {
    "externals": [
      "socket.io-client",
      "socket.io",
      "defu"
    ]
  },
  "scripts": {
    "prepare": "nuxt-module-build prepare",
    "prepack": "nuxt-module-build build",
    "build": "nuxi build-module",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run prepack && changelogen --release && npm publish --access public && git push --follow-tags",
    "lint": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.9.3",
    "h3": "^1.10.1",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.6",
    "@nuxt/eslint-config": "^0.5.3",
    "@nuxt/module-builder": "^0.8.3",
    "@nuxt/schema": "^3.6.5",
    "@nuxt/test-utils": "^3.13.1",
    "@pinia/nuxt": "^0.5.1",
    "@types/node": "^22.5.1",
    "changelogen": "^0.5.5",
    "defu": "^6.1.4",
    "eslint": "^9.4.0",
    "nuxt": "^3.13.0",
    "vitest": "^2.0.5"
  },
  "overrides": {
    "vue": "latest"
  }
}
